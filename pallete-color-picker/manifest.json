{
  "manifest_version": 3,
  "name": "Web Color Picker",
  "description": "Chrome Extension for business Users and developers to pick colors instantly from any webpage",
  "version": "1.0",
   "action":{
    "default_popup":"popup.html"
  },
  "icons": {
    "16": "images/picker_16.png",
    "32": "images/picker_32.png",
    "48": "images/picker_48.png"
  },
  "background":{
    "service_worker":"background.js"
  },
  "permissions":[
    "storage",
    "activeTab",
    "scripting"
  ]
}
